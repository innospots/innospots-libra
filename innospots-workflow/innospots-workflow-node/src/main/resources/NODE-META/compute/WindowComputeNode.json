{
  "name": "列字段扩展",
  "code": "win_compute",
  "flowCode": "EVENTS",
  "primitive": "compute",
  "vendor": "innospots",
  "description": "窗口滚动，累计，排序",
  "icon": "/image/NODE/win_compute",
  "nodeType": "io.innospots.workflow.node.app.compute.WindowComputeNode",
  "outPorts": "1",
  "inPorts": "1",
  "components": [
    {
      "name": "displayName",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "节点名称",
        "placeholder": "请输入节点名称",
        "initialValue": "列字段计算"
      }
    },
    {
      "name": "output_restricted",
      "type": "Switch",
      "valueType": "boolean",
      "copyable": false,
      "deletable": false,
      "settings": {
        "tooltip": "是否输出输入数据，等于 true 时，只会输出计算字段，输入数据不输出",
        "viewType": "switch",
        "initialValue": false,
        "label": "只输出计算字段",
        "default": false
      }
    },
    {
      "name": "func_type",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "dataSource": "static",
        "initialValue": "COLUMN",
        "label": "函数类型",
        "tooltip": "字段计算的方式",
		"viewType": "picker",
        "options": [
          {
            "value": "ROLLING",
            "label": "滚动计算"
          },
          {
            "value": "ACCUM",
            "label": "累计计算"
          },
          {
            "value": "COLUMN",
            "label": "列计算"
          }
        ]
      }
    },
    {
      "name": "rolling_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "win_compute_1",
        "label": "滚动计算字段",
        "tooltip": "按列采用滑动窗口使用函数计算新字段的值",
        "customColumns": [{
          "type": "INPUT",
          "width": "120px",
          "code": "fieldCode",
          "title": "字段名"
        }, {
          "code": "field",
          "type": "FIELD",
          "width": "120px",
          "title": "字段",
          "default": ""
        }, {
          "code": "function",
          "type": "SELECT",
          "width": "120px",
          "title": "函数",
          "default": "",
          "options": [{
            "label": "平均数",
            "value": "AVG"
          }, {
            "value": "SUM",
            "label": "求和"
          }, {
            "value": "MAX",
            "label": "最大值"
          }, {
            "value": "MIN",
            "label": "最小值"
          }, {
            "value": "STD_DEV",
            "label": "标准差"
          }, {
            "value": "VARIANCE",
            "label": "方差"
          }, {
            "value": "MAD",
            "label": "平均绝对差"
          }, {
            "value": "SOS",
            "label": "平方和"
          }, {
            "value": "SKEW",
            "label": "样本偏度"
          }, {
            "value": "KURT",
            "label": "样本峰度"
          }, {
            "value": "PROD",
            "label": "连乘"
          }]
        }, {
          "code": "winLength",
          "type": "INPUT",
          "width": "80px",
          "title": "窗口长度",
          "default": ""
        }, {
          "type": "FIELD",
          "width": "120px",
          "code": "weight",
          "title": "权重"
        }]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "ROLLING"
            }
          ]
        }
      ]
    },
    {
      "name": "accum_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "win_compute_1",
        "label": "累计计算字段",
        "tooltip": "基于选中列字段按滑动窗口采用函数累计计算值",
        "customColumns": [{
          "type": "INPUT",
          "width": "120px",
          "code": "fieldCode",
          "title": "字段名"
        }, {
          "code": "field",
          "type": "FIELD",
          "width": "120px",
          "title": "字段",
          "default": ""
        }, {
          "code": "function",
          "type": "SELECT",
          "width": "120px",
          "title": "函数",
          "default": "",
          "options": [{
            "label": "平均数",
            "value": "AVG"
          }, {
            "value": "SUM",
            "label": "求和"
          }, {
            "value": "MAX",
            "label": "最大值"
          }, {
            "value": "MIN",
            "label": "最小值"
          }, {
            "value": "STD_DEV",
            "label": "标准差"
          }, {
            "value": "VARIANCE",
            "label": "方差"
          }, {
            "value": "MAD",
            "label": "平均绝对差"
          }, {
            "value": "SOS",
            "label": "平方和"
          }, {
            "value": "SKEW",
            "label": "样本偏度"
          }, {
            "value": "KURT",
            "label": "样本峰度"
          }, {
            "value": "PROD",
            "label": "连乘"
          }]
        }, {
          "type": "FIELD",
          "width": "120px",
          "code": "weight",
          "title": "权重"
        }]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "ACCUM"
            }
          ]
        }
      ]
    },
    {
      "name": "shift_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "win_compute_1",
        "label": "移动计算字段",
        "tooltip": "按滑动函数移动计算新字段的值",
        "customColumns": [{
          "type": "INPUT",
          "width": "120px",
          "code": "fieldCode",
          "title": "字段名"
        }, {
          "code": "field",
          "type": "FIELD",
          "width": "120px",
          "title": "字段",
          "default": ""
        }, {
          "code": "function",
          "type": "SELECT",
          "width": "120px",
          "title": "函数",
          "default": "",
          "options": [{
            "label": "排名",
            "value": "RANK"
          }, {
            "value": "DIFF",
            "label": "差值"
          }, {
            "value": "SHIFT",
            "label": "滑动"
          }, {
            "value": "PCT_CHANGE",
            "label": "百分比变化"
          }]
        }, {
          "type": "INPUT",
          "width": "80px",
          "code": "winLength",
          "title": "窗口长度"
        }]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "COLUMN"
            }
          ]
        }
      ]
    }
  ]
}