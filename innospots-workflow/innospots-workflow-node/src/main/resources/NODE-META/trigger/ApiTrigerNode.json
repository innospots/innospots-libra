{
  "name": "Api触发器",
  "code": "WEBHOOK",
  "flowCode": "EVENTS",
  "primitive": "apiTrigger",
  "vendor": "innospots",
  "description": "api触发器节点",
  "icon": "/node/webhook.svg",
  "nodeType": "io.innospots.workflow.node.app.trigger.ApiTriggerNode",
  "outPorts": "1",
  "components": [
    {
      "name": "displayName",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "节点名称",
        "placeholder": "请输入节点名称",
        "initialValue": "api触发器"
      }
    },
    {
      "name": "path",
      "type": "WebhookInput",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "Webhook地址",
        "minLength": 4
      }
    },
    {
      "name": "requestType",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "请求方式",
		"viewType": "radio",
        "dataSource": "static",
        "initialValue": "GET",
        "options": [
          {"value": "GET", "label": "GET"},
          {"value": "POST", "label": "POST"}
        ]
      }
    },
    {
      "name": "responseMode",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "响应方式",
		"viewType": "radio",
        "dataSource": "static",
        "initialValue": "RESULT",
        "options": [
          {"value": "STREAM", "label": "流式返回"},
          {"value": "RESULT", "label": "返回工作流结果"}
        ]
      }
    }, {
      "name": "request_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "layout": {
        "h": 10, "minH": 6,
        "w": 1, "x": 0, "y": 0, "moved": true, "static": false,
        "resizeHandles": ["s", "n"]
      },
      "settings": {
        "label": "请求字段",
        "placeholder": "parameter field or body field",
        "initialValue": "",
        "customColumns": [
          {
            "type": "INPUT",
            "width": "140px",
            "code": "code",
            "title": "字段标识"
          },
          {
            "code": "name",
            "type": "INPUT",
            "width": "120px",
            "title": "字段名称",
            "default": ""
          },
          {
            "type": "INPUT",
            "width": "120px",
            "code": "value",
            "title": "默认值"
          }
        ]
      }
    }
  ]

}