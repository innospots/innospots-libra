{
  "name": "SQL",
  "code": "SQL",
  "flowCode": "EVENTS",
  "primitive": "connector",
  "vendor": "innospots",
  "credentialTypeCode": "SQL",
  "description": "SQL数据节点",
  "icon": "mysql",
  "nodeType": "io.innospots.workflow.node.app.connector.SqlDataNode",
  "outPorts": "1",
  "inPorts": "1",
  "components": [
    {
      "name": "displayName",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "节点名称",
        "placeholder": "请输入节点名称",
        "initialValue": "SQL数据节点"
      }
    },
    {
      "name": "credential_key",
      "type": "CredentialSelect",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "凭据",
        "showAddButton": true,
        "showEditButton": true
      }
    },
    {
      "name": "data_operation",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "操作方式",
		"viewType": "radio",
        "dataSource": "static",
        "initialValue": "GET_LIST",
        "optionType": "default",
        "options": [
          {"value": "GET_LIST", "label": "查询"},
          {"value": "INSERT", "label": "插入"},
          {"value": "UPDATE", "label": "更新"},
          {"value": "UPSERT", "label": "插入更新"}
        ]
      }
    },
    {
      "name": "sql_clause",
      "type": "ScriptEditor",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "查询语句",
        "scriptType": "SQL",
        "isFixedType": true
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "GET_LIST"
            }
          ]
        }
      ]
    },
    {
      "name": "table_name",
      "type": "DataTableSelect",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "数据表",
        "showAddButton": true,
        "showEditButton": true,
        "dependencies": "credential_key"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "INSERT"
            },
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "UPDATE"
            },
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "UPSERT"
            }
          ]
        }
      ]
    },
    {
      "name": "key_column",
      "type": "KeyFieldSelect",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "主键字段",
        "tableName": "table_name",
        "dependencies": "credential_key"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "UPSERT"
            }
          ]
        }
      ]
    },
    {
      "name": "query_conditions",
      "type": "QueryParams",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "查询参数",
        "widget": "QueryParams",
        "dependencies": "sql_clause"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "GET_LIST"
            }
          ]
        }
      ]
    },
    {
      "name": "update_conditions",
      "type": "FieldMapping",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "更新键",
        "fieldSource": "SQL_1",
        "tableName": "table_name",
        "bind": "",
        "dependencies": "credential_key"
      },
      "conditions": [
        {
          "result": "hidden",
          "relation": "OR",
          "children": [
            {
              "name": "data_operation",
              "opt": "UNEQUAL",
              "value": "UPDATE"
            }
          ]
        }
      ]
    },
    {
      "name": "updated_time",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "更新时间字段",
        "default": "updated_time"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "UPDATE"
            },
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "UPSERT"
            }
          ]
        }
      ]
    },
    {
      "name": "column_mapping",
      "type": "FieldMappingTable",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "字段映射",
        "fieldSource": "SQL_1",
        "tableName": "table_name",
        "dependencies": "credential_key",
        "columns": [
          {
            "width": "auto",
            "title": "表字段",
            "dataIndex": "name"
          },
          {
            "width": "auto",
            "title": "数据类型",
            "dataIndex": "valueType"
          },
          {
            "width": "160",
            "title": "映射字段",
            "dataIndex": "code"
          }
        ]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "data_operation",
              "opt": "UNEQUAL",
              "value": "GET_LIST"
            }
          ]
        }
      ]
    },
    {
      "name": "output_field_type",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "返回数据类型",
		"viewType": "radio",
        "dataSource": "static",
        "initialValue": "LIST",
        "options": [
          {"value": "LIST", "label": "列表"},
          {"value": "MAP", "label": "单条(对象)"}
        ]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "GET_LIST"
            }
          ]
        }
      ]
    },
    {
      "name": "output_mode_map",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "输出结果",
		"viewType": "picker",
        "optionsType": "customOptions",
        "dataSource": "static",
        "initialValue": "OVERWRITE",
        "options": [
          {"value": "FIELD", "label": "存入变量"},
          {"value": "PAYLOAD", "label": "与输入合并"},
          {"value": "OVERWRITE", "label": "只输出返回结果"}
        ]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "AND",
          "children": [
            {
              "name": "output_field_type",
              "opt": "EQUAL",
              "value": "MAP"
            },
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "GET_LIST"
            }
          ]
        }
      ]
    },
    {
      "name": "output_mode_list",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "输出结果",
        "optionsType": "customOptions",
        "dataSource": "static",
        "initialValue": "OVERWRITE",
		"viewType": "picker",
        "options": [
          {"value": "FIELD", "label": "存入变量"},
          {"value": "PAYLOAD", "label": "与输入合并"},
          {"value": "OVERWRITE", "label": "只输出返回结果"}
        ]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "AND",
          "children": [
            {
              "name": "output_field_type",
              "opt": "EQUAL",
              "value": "LIST"
            },
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "GET_LIST"
            }
          ]
        }
      ]
    },
    {
      "name": "variable_name",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "存储变量名称"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "AND",
          "children": [
            {
              "name": "output_mode_map",
              "opt": "EQUAL",
              "value": "FIELD"
            },
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "GET_LIST"
            }
          ]
        },
        {
          "result": "visible",
          "relation": "AND",
          "children": [
            {
              "name": "output_mode_list",
              "opt": "EQUAL",
              "value": "FIELD"
            },
            {
              "name": "data_operation",
              "opt": "EQUAL",
              "value": "GET_LIST"
            }
          ]
        }
      ]
    }
  ]
}