{
  "name": "python程序",
  "code": "cmdPython",
  "flowCode": "EVENTS",
  "primitive": "script",
  "vendor": "innospots",
  "description": "命令行形式执行 python",
  "icon": "/node/shell.svg",
  "nodeType": "io.innospots.workflow.node.app.script.CmdLinePythonNode",
  "outPorts": "1",
  "inPorts": "1",
  "components": [
    {
      "name": "displayName",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "节点名称",
        "placeholder": "请输入节点名称",
        "initialValue": "Python命令脚本"
      }
    },
    {
      "name": "cmd_path",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "脚本命令路径",
        "placeholder": "/usr/local/bin/python",
        "initialValue": ""
      }
    },
    {
      "name": "action_script",
      "type": "ScriptEditor",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "脚本详情",
        "scriptType": "PYTHON",
        "initialValue": "def process_item(item):\n  print(item)\n  return item",
        "isFixedType": true
      }
    },
    {
      "name": "output_mode",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "输出方式",
		"viewType": "picker",
        "dataSource": "static",
        "initialValue": "OVERWRITE",
        "options": [
          {
            "value": "FIELD",
            "label": "存入变量"
          },
          {
            "value": "PAYLOAD",
            "label": "合并输出"
          },
          {
            "value": "OVERWRITE",
            "label": "覆盖输出"
          },
          {
            "value": "LOG",
            "label": "日志输出"
          },
          {
            "value": "STREAM",
            "label": "流式日志"
          }
        ]
      }
    },
    {
      "name": "variable_name",
      "type": "ReadOnlyWidget",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "存储变量名称",
        "placeholder": "请输入"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "output_mode",
              "opt": "EQUAL",
              "value": "FIELD"
            }
          ]
        }
      ]
    }
  ]
}