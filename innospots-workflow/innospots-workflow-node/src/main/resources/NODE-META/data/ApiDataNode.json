{
  "name": "HTTP API",
  "code": "HTTPAPI",
  "primitive": "data",
  "vendor": "innospots",
  "description": "HttpAPI节点",
  "icon": "http-api",
  "nodeType": "io.innospots.workflow.node.app.data.ApiDataNode",
  "inPorts": "1",
  "outPorts": "1",
  "components": [
    {
      "name": "displayName",
      "type": "Input",
      "valueType": "string",
      "required": true,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "节点名称",
        "placeholder": "请输入节点名称",
        "initialValue": "HttpAPI节点"
      }
    },
    {
      "name": "auth_type",
      "type": "Select",
      "valueType": "string",
      "required": false,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "鉴权方式",
        "showAddButton": false,
        "showEditButton": false,
        "options": [
          {
            "value": "none",
            "label": "None"
          },
          {
            "value": "oauth2-auth-api",
            "label": "Oauth2 Auth"
          },
          {
            "value": "token-auth-api",
            "label": "Token Auth"
          }
        ]
      }
    },
    {
      "name": "credential",
      "type": "CredentialSelect",
      "valueType": "string",
      "required": false,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "凭据",
        "showAddButton": true,
        "showEditButton": true
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "auth_type",
              "opt": "NOTNULL"
            }
          ]
        }
      ]
    },
    {
      "name": "url_address",
      "type": "HttpURLInput",
      "valueType": "array",
      "required": true,
      "copyable": true,
      "deletable": true,
      "settings": {
        "label": "请求地址",
        "placeholder": "请输入请求地址"
      }
    },
    {
      "name": "request_params",
      "type": "HttpRequest",
      "valueType": "array",
      "required": true,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "HTTP请求参数",
        "isSelect": true,
        "dependencies": "url_address"
      }
    },
    {
      "name": "pre_action",
      "type": "CodeEditor",
      "valueType": "string",
      "required": false,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "前置脚本(JAVA)",
        "codeType": "JAVA",
        "isFixedType": true
      }
    },
    {
      "name": "action",
      "type": "CodeEditor",
      "valueType": "string",
      "required": false,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "结果脚本(JS)",
        "codeType": "JAVASCRIPT",
        "isFixedType": true,
        "default": "return item;"
      }
    },
    {
      "name": "output_payload",
      "type": "Switch",
      "valueType": "boolean",
      "required": false,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "返回数据是否存入Payload"
      }
    },
    {
      "name": "extract_path",
      "type": "Input",
      "valueType": "string",
      "required": false,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "数据提取",
        "default": "$"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "output_payload",
              "opt": "EQUAL",
              "value": "true"
            }
          ]
        }
      ]
    },
    {
      "name": "output_field_type",
      "type": "radio",
      "valueType": "string",
      "required": true,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "返回数据类型",
        "options": [
          {
            "value": "LIST",
            "label": "列表"
          },
          {
            "value": "MAP",
            "label": "单条(JSON对象)"
          }
        ],
        "default": "MAP",
        "props": {
          "optionType": "default"
        }
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "output_payload",
              "opt": "EQUAL",
              "value": "true"
            }
          ]
        }
      ]
    },
    {
      "name": "output_mode_map",
      "type": "Select",
      "valueType": "string",
      "required": true,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "存储方式",
        "default": "PAYLOAD",
        "options": [
          {
            "value": "FIELD",
            "label": "存入变量"
          },
          {
            "value": "PAYLOAD",
            "label": "合并入Payload"
          },
          {
            "value": "OVERWRITE",
            "label": "覆盖Payload"
          }
        ],
        "optionsType": "customOptions"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "output_field_type",
              "opt": "EQUAL",
              "value": "LIST"
            }
          ]
        },
        {
          "result": "hidden",
          "relation": "OR",
          "children": [
            {
              "name": "output_payload",
              "opt": "EQUAL",
              "value": "false"
            }
          ]
        }
      ]
    },
    {
      "name": "output_mode_list",
      "type": "Select",
      "valueType": "string",
      "required": true,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "存储方式",
        "default": "OVERWRITE",
        "options": [
          {
            "value": "FIELD",
            "label": "存入变量"
          },
          {
            "value": "OVERWRITE",
            "label": "覆盖Payload"
          }
        ],
        "optionsType": "customOptions"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "output_field_type",
              "opt": "EQUAL",
              "value": "MAP"
            }
          ]
        },
        {
          "result": "hidden",
          "relation": "OR",
          "children": [
            {
              "name": "output_payload",
              "opt": "EQUAL",
              "value": "false"
            }
          ]
        }
      ]
    },
    {
      "name": "variable_name",
      "type": "Input",
      "valueType": "string",
      "required": true,
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "存储变量名称",
        "default": "var_item"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "output_mode",
              "opt": "EQUAL",
              "value": "FIELD"
            }
          ]
        },
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "output_mode_2",
              "opt": "EQUAL",
              "value": "FIELD"
            }
          ]
        },
        {
          "result": "hidden",
          "relation": "OR",
          "children": [
            {
              "name": "output_payload",
              "opt": "EQUAL",
              "value": "false"
            }
          ]
        }
      ]
    }
  ]
}