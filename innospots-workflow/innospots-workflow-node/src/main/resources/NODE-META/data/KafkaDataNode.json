{
  "name": "Kafka数据节点",
  "code": "KAFKA",
  "flowCode": "EVENTS",
  "primitive": "trigger",
  "vendor": "innospots",
  "credentialTypeCode": "KAFKA",
  "description": "Kafka数据节点",
  "icon": "kafka",
  "nodeType": "io.innospots.workflow.node.app.data.KafkaDataNode",
  "inPorts": "1",
  "outPorts": "1",
  "components": [
    {
      "name": "displayName",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "节点名称",
        "placeholder": "请输入节点名称",
        "initialValue": "Kafka队列"
      }
    },
    {
      "name": "credential",
      "type": "CredentialSelect",
      "valueType": "number",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "凭据",
        "showAddButton": true,
        "showEditButton": true
      }
    },
    {
      "name": "registry_topic",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "Topic",
        "placeholder": "请输入Topic"
      }
    },
    {
      "name": "config_field",
      "type": "Switch",
      "valueType": "boolean",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "是否配置字段"
      }
    },
    {
      "name": "column_mapping",
      "type": "FieldMappingTable",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "字段映射",
        "tableName": "table_name",
        "dataSourceId": "datasource_id",
        "columns": [
          {
            "width": "50%",
            "title": "表字段",
            "dataIndex": "name"
          },
          {
            "width": "50%",
            "title": "映射字段",
            "dataIndex": "code"
          }
        ]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "config_field",
              "opt": "EQUAL",
              "value": "true"
            }
          ]
        }
      ]
    }
  ]

}