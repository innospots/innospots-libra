        "initialValue": "文件输出"
      }
    },
    {
      "name": "output_path",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "存储目录",
        "placeholder": "/home/user/store",
        "initialValue": ""
      }
    },
    {
      "name": "file_pattern",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "来源文件",
        "placeholder": "*.后缀名，例如: *.png",
        "initialValue": ".*"
