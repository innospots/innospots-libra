{
  "name": "分组计算",
  "code": "group_compute",
  "flowCode": "EVENTS",
  "primitive": "dataset",
  "vendor": "innospots",
  "description": "pandas transform计算，按 dim 字段分组列表，",
  "icon": "/image/NODE/group_compute",
  "nodeType": "io.innospots.workflow.node.app.dataset.GroupTransformNode",
  "outPorts": "1",
  "inPorts": "1",
  "components": [
    {
      "name": "displayName",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "节点名称",
        "placeholder": "请输入节点名称",
        "initialValue": "分组计算"
      }
    },
    {
      "name": "output_restricted",
      "type": "Switch",
      "valueType": "boolean",
      "copyable": false,
      "deletable": false,
      "settings": {
        "viewType": "switch",
        "initialValue": false,
        "label": "只输出计算字段"
      }
    },
    {
      "name": "source_field_type",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "聚合数据源",
		"viewType": "radio",
        "dataSource": "static",
        "initialValue": "payload",
        "options": [
          {
            "value": "payload",
            "label": "Payload"
          },
          {
            "value": "list",
            "label": "列表字段"
          }
        ]
      }
    },
    {
      "name": "list_parent_field",
      "type": "InputFieldSelect2",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "列表字段"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "source_field_type",
              "opt": "EQUAL",
              "value": "list"
            }
          ]
        }
      ]
    },
    {
      "name": "dim_field_list",
      "type": "InputFieldSelect2",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "分组字段"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "source_field_type",
              "opt": "EQUAL",
              "value": "list"
            }
          ]
        }
      ]
    },
    {
      "name": "dim_field_payload",
      "type": "InputFieldSelect2",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "分组字段"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "source_field_type",
              "opt": "EQUAL",
              "value": "payload"
            }
          ]
        }
      ]
    },
    {
      "name": "func_type",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "计算类型",
		"viewType": "picker",
        "dataSource": "static",
        "initialValue": "COLUMN",
        "options": [
          {
            "value": "ROLLING",
            "label": "滚动计算"
          },
          {
            "value": "ACCUM",
            "label": "累计计算"
          },
          {
            "value": "COLUMN",
            "label": "列计算"
          },
          {
            "value": "AGGRE",
            "label": "聚合计算"
          }
        ]
      }
    },
    {
      "name": "aggregate_field",
      "type": "AggregateVariables",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "聚合变量",
        "options": [
          {
            "label": "总计",
            "value": "SUM"
          },
          {
            "label": "次数",
            "value": "COUNT"
          },
          {
            "label": "非重复计数",
            "value": "DISCOUNT"
          },
          {
            "label": "平均值",
            "value": "AVG"
          },
          {
            "label": "最大值",
            "value": "MAX"
          },
          {
            "label": "最小值",
            "value": "MIN"
          },
          {
            "label": "标准差",
            "value": "STD_DEV"
          },
          {
            "label": "中位数",
            "value": "MEDIAN"
          },
          {
            "label": "众数",
            "value": "MODE"
          },
          {
            "label": "无偏方差",
            "value": "VARIANCE"
          },
          {
            "label": "均值的标准误差",
            "value": "SEM"
          },
          {
            "label": "平均绝对差",
            "value": "MAD"
          },
          {
            "label": "样本偏度（第三阶）",
            "value": "SKEW"
          },
          {
            "label": "样本偏度（第四阶）",
            "value": "KURT"
          },
          {
            "label": "平方和",
            "value": "SOS"
          },
          {
            "label": "半方差",
            "value": "SEMI_VAR"
          },
          {
            "label": "连乘",
            "value": "PROD"
          },
          {
            "label": "总体标准差",
            "value": "POP_STD_DEV"
          },
          {
            "label": "总体方差",
            "value": "POP_VARIANCE"
          },
          {
            "label": "几何平均数",
            "value": "GEOMETRIC_MEAN"
          },
          {
            "label": "log求和",
            "value": "SUM_OF_LOGS"
          }
        ]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "AGGRE"
            }
          ]
        }
      ]
    },
    {
      "name": "rolling_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "滚动计算字段",
        "tooltip": "滚动计算字段值，并输出到结果集"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "ROLLING"
            }
          ]
        }
      ]
    },
    {
      "name": "accum_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "聚合计算字段",
        "tooltip": "聚合计算字段值"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "ACCUM"
            }
          ]
        }
      ]
    },
    {
      "name": "shift_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "移动列计算字段",
        "tooltip": "移动列计算字段"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "COLUMN"
            }
          ]
        }
      ]
    }
  ]
}