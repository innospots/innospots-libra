{
  "name": "分组计算",
  "code": "group_compute",
  "flowCode": "EVENTS",
  "primitive": "dataset",
  "vendor": "innospots",
  "description": "pandas transform计算，按 dim 字段分组列表，",
  "icon": "/image/NODE/group_compute",
  "nodeType": "io.innospots.workflow.node.app.dataset.GroupTransformNode",
  "outPorts": "1",
  "inPorts": "1",
  "components": [
    {
      "name": "displayName",
      "type": "Input",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "节点名称",
        "placeholder": "请输入节点名称",
        "initialValue": "分组计算"
      }
    },
    {
      "name": "source_field_type",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "聚合数据源",
		"viewType": "radio",
        "dataSource": "static",
        "initialValue": "payload",
        "hidden": true,
        "options": [
          {
            "value": "payload",
            "label": "输入数据"
          },
          {
            "value": "list",
            "label": "列表字段"
          }
        ]
      }
    },
    {
      "name": "list_parent_field",
      "type": "InputFieldSelect2",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "列表字段"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "source_field_type",
              "opt": "EQUAL",
              "value": "list"
            }
          ]
        }
      ]
    },
    {
      "name": "dim_field_list",
      "type": "InputFieldSelect2",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "multiple": true,
        "fieldSource": "group_compute_1",
        "label": "分组字段"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "source_field_type",
              "opt": "EQUAL",
              "value": "list"
            }
          ]
        }
      ]
    },
    {
      "name": "dim_field_payload",
      "type": "InputFieldSelect2",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "multiple": true,
        "fieldSource": "group_compute_1",
        "label": "分组字段"
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "source_field_type",
              "opt": "EQUAL",
              "value": "payload"
            }
          ]
        }
      ]
    },
    {
      "name": "output_restricted",
      "type": "Switch",
      "valueType": "boolean",
      "copyable": false,
      "deletable": false,
      "settings": {
        "tooltip": "是否输出输入数据，等于 true 时，只会输出计算字段，输入数据不输出",
        "viewType": "switch",
        "initialValue": false,
        "label": "只输出计算字段",
        "default": false
      }
    },
    {
      "name": "func_type",
      "type": "OptionPicker",
      "valueType": "string",
      "copyable": false,
      "deletable": false,
      "settings": {
        "label": "计算类型",
		"viewType": "picker",
        "dataSource": "static",
        "initialValue": "COLUMN",
        "options": [
          {
            "value": "ROLLING",
            "label": "滚动计算"
          },
          {
            "value": "ACCUM",
            "label": "累计计算"
          },
          {
            "value": "COLUMN",
            "label": "列计算"
          }
        ]
      }
    },
    {
      "name": "rolling_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "滚动计算字段",
        "tooltip": "滚动计算字段值，并输出到结果集",
        "customColumns": [{
          "type": "INPUT",
          "width": "120px",
          "code": "fieldCode",
          "title": "字段名"
        }, {
          "code": "field",
          "type": "FIELD",
          "width": "120px",
          "title": "字段",
          "default": ""
        }, {
          "code": "function",
          "type": "SELECT",
          "width": "120px",
          "title": "函数",
          "default": "",
          "options": [{
            "label": "平均数",
            "value": "AVG"
          }, {
            "value": "SUM",
            "label": "求和"
          }, {
            "value": "MAX",
            "label": "最大值"
          }, {
            "value": "MIN",
            "label": "最小值"
          }, {
            "value": "STD_DEV",
            "label": "标准差"
          }, {
            "value": "VARIANCE",
            "label": "方差"
          }, {
            "value": "MAD",
            "label": "平均绝对差"
          }, {
            "value": "SOS",
            "label": "平方和"
          }, {
            "value": "SKEW",
            "label": "样本偏度"
          }, {
            "value": "KURT",
            "label": "样本峰度"
          }, {
            "value": "PROD",
            "label": "连乘"
          }]
        }, {
          "code": "winLength",
          "type": "INPUT",
          "width": "80px",
          "title": "窗口长度",
          "default": ""
        }, {
          "type": "FIELD",
          "width": "120px",
          "code": "weight",
          "title": "权重"
        }]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "ROLLING"
            }
          ]
        }
      ]
    },
    {
      "name": "accum_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "累计计算字段",
        "tooltip": "累计计算字段值",
        "customColumns": [{
          "type": "INPUT",
          "width": "120px",
          "code": "fieldCode",
          "title": "字段名"
        }, {
          "code": "field",
          "type": "FIELD",
          "width": "120px",
          "title": "字段",
          "default": ""
        }, {
          "code": "function",
          "type": "SELECT",
          "width": "120px",
          "title": "函数",
          "default": "",
          "options": [{
            "label": "平均数",
            "value": "AVG"
          }, {
            "value": "SUM",
            "label": "求和"
          }, {
            "value": "MAX",
            "label": "最大值"
          }, {
            "value": "MIN",
            "label": "最小值"
          }, {
            "value": "STD_DEV",
            "label": "标准差"
          }, {
            "value": "VARIANCE",
            "label": "方差"
          }, {
            "value": "MAD",
            "label": "平均绝对差"
          }, {
            "value": "SOS",
            "label": "平方和"
          }, {
            "value": "SKEW",
            "label": "样本偏度"
          }, {
            "value": "KURT",
            "label": "样本峰度"
          }, {
            "value": "PROD",
            "label": "连乘"
          }]
        }, {
          "type": "FIELD",
          "width": "120px",
          "code": "weight",
          "title": "权重"
        }]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "ACCUM"
            }
          ]
        }
      ]
    },
    {
      "name": "shift_fields",
      "type": "FieldList",
      "valueType": "array",
      "copyable": false,
      "deletable": false,
      "settings": {
        "fieldSource": "group_compute_1",
        "label": "移动列计算字段",
        "tooltip": "移动列计算字段",
        "customColumns": [{
          "type": "INPUT",
          "width": "120px",
          "code": "fieldCode",
          "title": "字段名"
        }, {
          "code": "field",
          "type": "FIELD",
          "width": "120px",
          "title": "字段",
          "default": ""
        }, {
          "code": "function",
          "type": "SELECT",
          "width": "120px",
          "title": "函数",
          "default": "",
          "options": [{
            "label": "排名",
            "value": "RANK"
          }, {
            "value": "DIFF",
            "label": "差值"
          }, {
            "value": "SHIFT",
            "label": "滑动"
          }, {
            "value": "PCT_CHANGE",
            "label": "百分比变化"
          }]
        }, {
          "type": "INPUT",
          "width": "80px",
          "code": "winLength",
          "title": "窗口长度"
        }]
      },
      "conditions": [
        {
          "result": "visible",
          "relation": "OR",
          "children": [
            {
              "name": "func_type",
              "opt": "EQUAL",
              "value": "COLUMN"
            }
          ]
        }
      ]
    }
  ]
}